<body>
    <!-- Navbar -->
    <%- include('includes/navbar') %>
    
    <div class="mt-5"></div>

    <div class="container">
        
        <div class="row justify-content-center">
            <div class="col-8">
                <h1 class="text-center">Payment Complete!</h1>
                <h3 class="text-center">Thanks for the purchase <%= payerinfo.name %>!</h3>
                <div class="steps">
                    <ol>
                        <li id="step1" class="steps-completed">
                            <div class="material-icons steps-icon">check</div>
        
                            Step one
                            <div class="steps-sub-title">Date Range</div>
                        </li>
                        <li id="step2" class="steps-completed">
                            <div class="material-icons steps-icon">check</div>Step two
                            <div class="steps-sub-title">Authentication</div>
                        </li>
                        <li id="step3" class="steps-completed">
                            <div class="material-icons steps-icon">check</div>Step three
                            <div class="steps-sub-title">Payment</div>
                        </li>
                        <li id="step4" class="steps-completed">
                            <div class="material-icons steps-icon">check</div>Finished
                        </li>
                    </ol>
                    <div class="steps-responsive">
                        <div class="steps-responsive-text">Finished</div>
                    </div>
                </div>
                <div class="mt-5"></div>
                <div class="row justify-content-center">
                    <div class="col">
                        <h4>Shipping details</h4>
                        <p>Address: <%= payerinfo.shippingAddress.addressee %> <%= payerinfo.shippingAddress.addressee %></p>
                        <p>Street: <%= payerinfo.shippingAddress.streetAddress %></p>
                        <p>Postal: <%= payerinfo.shippingAddress.city %> <%= payerinfo.shippingAddress.zipCode %></p>
                    </div>
                    <div class="col">
                        <h4>Order details</h4>
                        <p>Email: <%= payerinfo.email %></p>
                        <p>Phone: <%= payerinfo.msisdn %></p>
                        <p>IP: <%= payerinfo.device.ipAddress %> (<%= payerinfo.device.platformName %>)</p>
                    </div>
                </div>
               
                <div class="mt-5"></div>
                <h3>Ordered Items:</h3>
                <table class="table">
                    <thead>
                      <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                        <th scope="col">Price</th>
                        <th scope="col">Amount</th>
                        <th scope="col">Total</th>
                      </tr>
                    </thead>
                    <tbody>
                        
                        <% 
                        var totalPrice = Number()
                        items.forEach(data => { 

                            %>
                            <tr>
                                <th scope="row"><%= data.reference %></th>
                                <td><%= data.name %></td>
                                <td><%= data.unitPrice / 100 %> NOK</td>
                                <td><%= data.quantity %></td>
                                <td><%= data.amount / 100 %> NOK</td>
                            </tr>
                            <% totalPrice += data.amount / 100
                        %>

                            
                        <% }) %>
                        <tr>
                            <th></th>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td><%= totalPrice %> NOK</td>
                        </tr>
                    </tbody>
                    

                </table>
            </div>
        </div>
    </div>
    <script src="https://design.swedbankpay.com/v/8.0.1/scripts/dg.js" global="true" autoload="true"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
